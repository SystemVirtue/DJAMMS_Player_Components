{
  "name": "djamms-player",
  "version": "2.0.0",
  "description": "DJAMMS Video Player - Crossfading video player with search and browse functionality",
  "main": "electron/main.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "electron"
  ],
  "scripts": {
    "setup:logging": "node scripts/setup-logging.js",
    "dev": "npm run setup:logging && bash scripts/close-ports.sh && concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron-main && wait-on http://localhost:3003 && cross-env NODE_ENV=development electron .",
    "dev:electron:debug": "npm run build:electron-main && wait-on http://localhost:3003 && cross-env NODE_ENV=development electron . --remote-debugging-port=9222",
    "build": "npm run build:vite && npm run build:rollup && npm run build:electron-main",
    "build:vite": "vite build",
    "build:rollup": "rollup -c",
    "build:electron-main": "tsc -p electron/tsconfig.json",
    "build:electron": "npm run build:vite && npm run build:electron-main && electron-builder",
    "build:electron:mac": "npm run build:vite && npm run build:electron-main && electron-builder --mac",
    "build:electron:win": "npm run build:vite && npm run build:electron-main && electron-builder --win",
    "build:electron:linux": "npm run build:vite && electron-builder --linux",
    "start": "electron .",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "postinstall": "electron-builder install-app-deps",
    "dev:kiosk": "npm run dev --prefix web/kiosk",
    "dev:admin": "npm run dev --prefix web/admin",
    "build:kiosk": "npm run build --prefix web/kiosk",
    "build:admin": "npm run build --prefix web/admin",
    "install:web": "npm install --prefix web/kiosk && npm install --prefix web/admin",
    "supabase": "supabase",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link",
    "supabase:db:push": "supabase db push",
    "supabase:db:reset": "supabase db reset",
    "supabase:migration:new": "supabase migration new",
    "supabase:migration:list": "supabase migration list",
    "supabase:status": "supabase status"
  },
  "keywords": [
    "react",
    "video-player",
    "crossfade",
    "electron",
    "djamms",
    "music-player"
  ],
  "author": "DJAMMS Team",
  "license": "MIT",
  "type": "module",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "supabase": "^2.66.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.6",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.0",
    "wait-on": "^9.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/djamms-player-react.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/djamms-player-react/issues"
  },
  "homepage": "https://github.com/your-org/djamms-player-react#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "axios": "^1.13.2",
    "electron-store": "^11.0.2",
    "fast-deep-equal": "^3.1.3",
    "fuse.js": "^7.1.0",
    "react-router-dom": "^7.9.6"
  },
  "build": {
    "appId": "com.djamms.player",
    "productName": "DJAMMS Player",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "public/**/*",
      "package.json",
      "node_modules/**/*",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!node_modules/*.d.ts",
      "!node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{thumbs.db,Thumbs.db,.eslintrc.js,.eslintrc.cjs,.prettierrc}"
    ],
    "asar": true,
    "extraResources": [
      {
        "from": "dist",
        "to": "app/dist"
      }
    ],
    "mac": {
      "category": "public.app-category.music",
      "icon": "public/icon.png",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "icon": "public/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "signAndEditExecutable": false
    },
    "linux": {
      "icon": "public/icon.png",
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Audio"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
